{
	"files.exclude": {
		"node_modules": true,
	},
	"eslint.validate": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact",
	],
	"eslint.format.enable": true,
	"[typescript]": {
		"editor.formatOnSave": true
	},
	"editor.renderWhitespace": "boundary",
	"editor.insertSpaces": false,
	"typescript.tsdk": "node_modules/typescript/lib",
	"search.exclude": {
		"node_modules": true,
		".output": true,
		"lib": true,
	},
	"cSpell.words": [
		"residualeffect"
	],
}